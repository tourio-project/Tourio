import 'transport_type_enum.dart';

class ActivityCard {
  final String image;
  final String title;
  final String description;
  final String location;
  final String duration;
  final List<TransportTypeEnum> transport;

  ActivityCard({
    required this.image,
    required this.title,
    required this.description,
    required this.location,
    required this.duration,
    required this.transport,
  });
}
